                    $('.slider-right02').slick({
                        infinite: true,
                        speed: 700,
                        fade: true,
                        autoplay: true,
                        slidesToShow: 1,
                        adaptiveHeight: true
                    });

                    // Add background opacity for product-item
                    $(".block-Service").hover(function () {
                        $(this).find('.over').stop().fadeTo("fast", 1);
                    },
                      function () {
                          $(this).find('.over').stop().fadeTo("fast", 0);
                      });


                });
            </script>
