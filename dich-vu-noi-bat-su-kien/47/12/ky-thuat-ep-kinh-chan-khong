            <script type="text/javascript">
                function ResolveUrl(url) {
                    if (url.indexOf("~/") == 0) {
                        url = baseUrl + url.substring(2);
                    }
                    return url;
                }
                var baseUrl = "/tra-cuu-dich-vu/";
                function SearchData() {
                    var values = document.getElementById('ContentHeaderTop_txt_Search');
                if (values.value.length == 0)
                    return;
                var link = baseUrl + values.value.replace("/", "");

                window.location.href = link;
            }
            function runScript2(e) {

                if (event.which == 13 || event.keyCode == 13) {

                    SearchData();
                    window.event.returnValue = false;
                }
            }
            function buttonSend() {
                var hoten = document.getElementById("Contact_HoTen").value;
                var dienthoai = document.getElementById("Contact_DienThoai").value;
                var email = document.getElementById("Contact_Email").value;
                var tieude = document.getElementById("Contact_TieuDe").value;
                var noidung = document.getElementById("Contact_NoiDung").value;
                SendMail(hoten, dienthoai, email, tieude, noidung);

            }
            function SendMail(hoten, dienthoai, email, tieude, noidung) {
                var URLSite = "VienDiDongSolution.asmx/SendMail";
                $(".modal").show();
                $.ajax({
                    url: URLSite,

                    type: 'POST',
                    data: { _Hoten: hoten, _DienThoai: dienthoai, _Email: email, _TieuDe: tieude, _NoiDung: noidung },
                    dataType: "text",

                    success: function (resp) {
                        var repsValues = resp;
                        if (resp == null)
                            repsValues = resp.d;
                        var kqCT = repsValues.toString().replace("<?xml version=\"1.0\" encoding=\"utf-8\"?>", "").replace("<string xmlns=\"http://tempuri.org/\">", "").replace("</string>", "").replace("<string xmlns=\"http://tempuri.org/\" />", "");  //3.5  
                        alert(kqCT);
                        document.getElementById("Contact_HoTen").value = "";
                        document.getElementById("Contact_DienThoai").value = "";
                        document.getElementById("Contact_Email").value = "";
                        document.getElementById("Contact_TieuDe").value = "";
                        document.getElementById("Contact_NoiDung").value = "";
                        $(".modal").hide();
                        document.getElementById("box_mail1").style = "block";
                    }
               ,
                    error: function (e) {
                        alert('Error: ' + e);
                        $(".modal").hide();
                    }
                });

            }
            </script>
